#!/usr/bin/env fish

set source_dir ~/Projects/effect-start
set target_dir ./node_modules/effect-start

if test -d $target_dir
    echo "Syncing $source_dir to $target_dir"
    rsync -a --delete --exclude-from=$source_dir/.gitignore $source_dir/ $target_dir/
else
    echo "Target directory $target_dir does not exist"
end
