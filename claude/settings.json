{
 "defaultMode": "acceptEdits",
  "env": {
    "DISABLE_AUTOUPDATER": "0"
  },
  "permissions": {
    "allow": [
      "Bash(bun run:*)",
      "Bash(bun test:*)",
      "Bash(curl:*)",
      "Bash(find:*)",
      "Bash(gh:*)",
      "Bash(grep:*)",
      "Bash(lsof:*)",
      "Bash(xargs kill:*)",
      "Bash(dprint fmt:*)",
      "Bash(git fetch)",
      "Bash(git pull)",
      "Bash(git commit)",
      "Read(~/.zshrc)"
    ],
    "deny": []
  },
  "enabledPlugins": {
    "commit-commands@claude-code-plugins": true,
    "dprint-formatter@local-plugins": true
  },
  "alwaysThinkingEnabled": true,
  "showWelcomeMessage": true,
  "editorMode": "vim"
}
