{
  "showWelcomeMessage": true,
  "editorMode": "vim",
  "permissions": {
    "allow": [
      "Bash(bun run lint)",
      "Bash(bun test:*)",
      "Bash(find)",
      "Bash(gh:*)",
      "Bash(grep:*)",
      "Read(~/.zshrc)"
    ],
    "deny": []
  },
  "env": {
    "DISABLE_AUTOUPDATER": "0"
  },
  "alwaysThinkingEnabled": false,
  "defaultMode": "acceptEdits"
}
